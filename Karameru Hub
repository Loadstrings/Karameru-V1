-- Scripts:

local function YSWI_fake_script() -- script.LocalScript 
	local script = Instance.new('LocalScript', script)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.HubScroll.Visible = false
		script.Parent.Parent.scriptscroll.Visible = true
	end)
	
end
coroutine.wrap(YSWI_fake_script)()
local function EZEPAN_fake_script() -- Hubs.LocalScript 
	local script = Instance.new('LocalScript', Hubs)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.HubScroll.Visible = true
		script.Parent.Parent.scriptscroll.Visible = false
	end)
	
end
coroutine.wrap(EZEPAN_fake_script)()
local function UTKHXL_fake_script() -- ScreenGui.Gui stuff 
	local script = Instance.new('Script', ScreenGui)

	--//Items\\-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	local gui = script.Parent
	local mainframe = gui.MainFrame
	local titleframe = mainframe.TitleFrame
	mainframe.Active = true
	mainframe.Selectable = true
	mainframe.Draggable = true
	local scrollframe = mainframe.scriptscroll
	local hubscroll = mainframe.HubScroll
	--//Printing\\--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	print("Karameru V1 Executed! Welcome!")
	--//Functions\\-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	titleframe.TextButton.MouseButton1Click:Connect(function()
		gui:Destroy()
	end)
	
	scrollframe.Saitama.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/Loadstrings/Saitama/main/Saitama.txt'),true))()
	end)
	
	hubscroll.BeachHub.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/bedra45/BeachHub/main/BeachAnimation'),true))()
	end)
	
	hubscroll.Amazement.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/followz/Amazement/main/Amazement%20V2'),true))()
	end)
	
	hubscroll.UTG.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Blukez/Scripts/main/UTG%20V3%20RAW"))()
	end)
	
	scrollframe.BlockGun.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/Loadstrings/Block-gun/main/Gun.txt'),true))()
	end)
	
	scrollframe.Butterloaf.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/Loadstrings/Butterloaf/main/Butterloaf.txt'),true))()
	end)
	
	hubscroll.YBAHub.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://pastebin.com/raw/XUSatpwK'),true))()
	end)
end
coroutine.wrap(UTKHXL_fake_script)()
