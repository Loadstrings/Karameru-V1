coroutine.wrap(JWPS_fake_script)()
local function DHFLFU_fake_script() -- keyframe.LocalScript 
	local script = Instance.new('LocalScript', keyframe)

	local key = script.Parent.TextBox
	local validkeys = "karameru"
	
	script.Parent.TextButton.MouseButton1Click:Connect(function()
		if key.Text == validkeys then
			script.Parent.Parent.MainFrame.Visible = true
			wait(0.00001)
			script.Parent:Destroy()
		elseif key.Text == "" then
			key.Text = "" 
		else
			key.Text = "Incorrect"
			wait(1)
			key.Text = ""
		end
	end)
end
coroutine.wrap(DHFLFU_fake_script)()
