-- Scripts:

local function HJKIAA_fake_script() -- Hubs.LocalScript 
	local script = Instance.new('LocalScript', Hubs)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Home.Visible = false
		script.Parent.Parent.HubScroll.Visible = true
		script.Parent.Parent.scriptscroll.Visible = false
	end)
	
end
coroutine.wrap(HJKIAA_fake_script)()
local function YBYEK_fake_script() -- Homebutton.LocalScript 
	local script = Instance.new('LocalScript', Homebutton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Home.Visible = true
		script.Parent.Parent.HubScroll.Visible = false
		script.Parent.Parent.scriptscroll.Visible = false
	end)
	
end
coroutine.wrap(YBYEK_fake_script)()
local function VNTCSQE_fake_script() -- Home.LocalScript 
	local script = Instance.new('LocalScript', Home)

	local frame = script.Parent
	local player = game.Players.LocalPlayer
	local userid = player.UserId
	local thumtype = Enum.ThumbnailType.HeadShot
	local thumsize = Enum.ThumbnailSize.Size420x420
	local content = game.Players:GetUserThumbnailAsync(userid, thumtype, thumsize)
	
	frame.ImageLabel.Image = content
	frame.TextLabel.Text = "Welcome, ".. player.Name 
end
coroutine.wrap(VNTCSQE_fake_script)()
local function EXADLVK_fake_script() -- script.LocalScript 
	local script = Instance.new('LocalScript', script)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Home.Visible = false
		script.Parent.Parent.HubScroll.Visible = false
		script.Parent.Parent.scriptscroll.Visible = true
	end)
	
end
coroutine.wrap(EXADLVK_fake_script)()
local function JWPS_fake_script() -- ScreenGui.Gui stuff 
	local script = Instance.new('Script', ScreenGui)

	--//Items\\-----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	local gui = script.Parent
	local mainframe = gui.MainFrame
	local titleframe = mainframe.TitleFrame
	mainframe.Active = true
	mainframe.Selectable = true
	mainframe.Draggable = true
	local scrollframe = mainframe.scriptscroll
	local hubscroll = mainframe.HubScroll
	local home = mainframe.Home
	--//Printing\\--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	print("Karameru V1 Executed! Welcome!")
	--//Functions\\-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
	titleframe.ImageButton.MouseButton1Click:Connect(function()
		hubscroll.Visible = false
		home.Visible = false
		scrollframe.Visible = false
		mainframe.Homebutton.Visible = false
		mainframe.Hubs.Visible = false
		mainframe.script.Visible = false
		mainframe.BackgroundTransparency = 1
		titleframe.OpenButton.Visible = true
		titleframe.ImageButton.Visible = false
	end)
	
	titleframe.OpenButton.MouseButton1Click:Connect(function()
		hubscroll.Visible = true
		home.Visible = true
		scrollframe.Visible = true
		mainframe.Homebutton.Visible = true
		mainframe.Hubs.Visible = true
		mainframe.script.Visible = true
		mainframe.BackgroundTransparency = 0
		titleframe.OpenButton.Visible = false
		titleframe.ImageButton.Visible = true
	end)
	
	mainframe.Home.Teleport.MouseButton1Click:Connect(function()
		local p1 = game.Players.LocalPlayer.Character.HumanoidRootPart
		local p2 = home.Target.Text
		local pos = p1.CFrame
	
		p1.CFrame = game.Players[p2].Character.HumanoidRootPart.CFrame
	end)
	
	hubscroll.Linorix.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/vilRBLX/Linorix/main/.github/workflows/linorix.lua'),true))()
	end)
	
	hubscroll.DomainX.MouseButton1Click:Connect(function()
