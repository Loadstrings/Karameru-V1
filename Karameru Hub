    Text = "Domain X",
    Callback = function()
        loadstring(game:HttpGet(('https://shlex.dev/release/domainx/latest.lua'),true))()
    end
})

Page3.Button({
    Text = "Amazement",
    Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/followz/Amazement/main/Amazement%20V2'),true))()
    end
})
-- Netless

Page4.Button({
    Text = "Netless",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/Cu7bKQWN"))()
    end
})
