TextButton.TextWrapped = true

-- Scripts:

local function RFVDY_fake_script() -- ScreenGui.Script 
	local script = Instance.new('Script', ScreenGui)

	local gui = script.Parent
	local mainframe = gui.MainFrame
	local titleframe = mainframe.TitleFrame
	mainframe.Active = true
	mainframe.Selectable = true
	mainframe.Draggable = true
	
	print("Karameru V1 Executed! Welcome!")
	
	titleframe.TextButton.MouseButton1Click:Connect(function()
		gui:Destroy()
	end)
	
end
coroutine.wrap(RFVDY_fake_script)()
