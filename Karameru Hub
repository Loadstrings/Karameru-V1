local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local karameruuser = game.Players.LocalPlayer

local UI = Material.Load({
     Title = "Karameru Hub V1", 
     Style = 3,
     SizeX = 550, -- Size in X
     SizeY = 350,  -- Size in Y
     Theme = "Aqua", -- There are 5 Styles, Aqua, Mocha, Jester, Dark, Light
     ColorOverrides = {
        MainFrame = Color3.fromRGB(35, 35, 35)
    }
})

-- Pages

local Page = UI.New({
    Title = "Home" 
})

local Page2 = UI.New({
    Title = "Scripts" 
})

local Page3 = UI.New({
    Title = "Hubs" 
})

local Page4 = UI.New({
    Title = "Netless" 
})

--Home

Page.Label({
    Text = "Welcome, ".. karameruuser.Name
})

Page.Slider({
    Text = "Walkspeed",
    Callback = function(value)
        karameruuser.Character.Humanoid.WalkSpeed = (value)
    end,
    Min = 1,
    Max = 1000,
    Def = 16
})

Page.Slider({
    Text = "Jumppower",
    Callback = function(value)
        karameruuser.Character.Humanoid.JumpPowerw = (value)
    end,
    Min = 1,
    Max = 1000,
    Def = 50
})

Page.Button({
    Text = "Anti Fling",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Loadstrings/AF/main/Anti%20Fling.txt"))()
    end
})

Page.Button({
    Text = "Anti Stroller",
    Callback = function()
        local rs = game:GetService("RunService")
		print("anti stroller activated, get those noobs!")
		rs.Heartbeat:Connect(function()
			game.Players.LocalPlayer.Character.Humanoid.Sit = false
		end)
    end
})

-- Scripts

Page2.Button({
    Text = "AJ Blaster",
    Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Loadstrings/AJ/main/Aj%20Blaster%20Script.lua'),true))()
    end
})

Page2.Button({
    Text = "Killer",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Loadstrings/KI/main/Nep.%20sword%20and%20gun.lua"))()
    end
})

Page2.Button({
    Text = "Lightning Cannon",
    Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Loadstrings/LC/main/Lightning%20Cannon.lua'),true))()
    end
})

Page2.Button({
    Text = "Grab Knife",
    Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Loadstrings/GK/main/Grab%20Knife.lua'),true))()
    end
})

-- Hubs

Page3.Button({
    Text = "Pendulum Hub",
    Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/Tescalus/Pendulum-Hubs-Source/main/Pendulum%20Hub%20V4.lua'),true))()
    end
})

Page3.Button({
    Text = "Beach Hub",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/bedra45/BeachHub/main/BeachAnimation"))()
    end
})

